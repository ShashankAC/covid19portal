(states)\r\n    }\r\n    \r\n    return (\r\n        <div className={styles.container}>\r\n            {/* <div className={styles.states}>\r\n                <label className={styles.selectAll}>Select All</label>\r\n                    <input \r\n                        type=\"checkbox\"\r\n                        name=\"selectAll\"\r\n                        value={selectAll}\r\n                        checked={selectAll}\r\n                        onChange={handleChange}\r\n                    />\r\n            </div> */}\r\n            {statesList.map((state, i) => (\r\n                <div key={i} className={styles.states}>\r\n                    <label className={styles.statesName}>{state.name}</label>\r\n                    <input  \r\n                        type=\"checkbox\" \r\n                        value={state.name}\r\n                        checked={state.isChecked} \r\n                        name={state.name}\r\n                        className={styles.stateCheckBox}\r\n                        onChange={handleChange}/>\r\n                </div>\r\n            ))}\r\n        </div>\r\n    )\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    states: state.states\r\n})\r\n\r\nconst mapDispatchToProps = {\r\n    onUpdateStates: updateStates\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Navbar)","D:\\Covid19Portal\\covid19portal\\src\\containers\\MainContent\\MainContent.jsx",["64"],"D:\\Covid19Portal\\covid19portal\\src\\Store\\reducers\\statesReducers.js",[],"D:\\Covid19Portal\\covid19portal\\src\\Store\\reducers\\dateReducers.js",[],"D:\\Covid19Portal\\covid19portal\\src\\Store\\actions\\actionTypes.js",[],"D:\\Covid19Portal\\covid19portal\\src\\Store\\actions\\statesAction.js",[],"D:\\Covid19Portal\\covid19portal\\src\\Store\\actions\\dataAction.js",[],"D:\\Covid19Portal\\covid19portal\\src\\components\\BarChart\\BarChart.jsx",[],["65","66"],"D:\\Covid19Portal\\covid19portal\\src\\components\\BarChart\\BarChartD3.js",[]